{
  "name": "Kano",
  "version": "0.0.1",
  "description": "Simple tab manager",
  "permissions": [
    "tabs",
    "favicon"
  ],
  "action": {
    "default_icon": {
      "32": "images/kano32.png"
    },
    "default_popup": "index.html",
    "default_title": "Kano"
  },
  "icons": {
    "16": "/images/kano16.png",
    "32": "/images/kano32.png",
    "48": "/images/kano48.png",
    "128": "/images/kano128.png",
    "256": "/images/kano256.png"
  },
  "manifest_version": 3
}
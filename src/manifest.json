{
  "name": "ninja",
  "version": "1.0",
  "description": "Manage tabs with ease",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_icon": "images/tabble16.png",
    "default_popup": "index.html",
    "default_title": "ninja popup"
  },
  "manifest_version": 3
}